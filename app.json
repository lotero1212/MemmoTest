{
  "expo": {
    "name": "MemmoTest",
    "slug": "MemmoTest",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/MemmIcon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/MemmSplash.png",
      "resizeMode": "cover"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/MemmIcon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.lucasotee.MemmoTest"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "79e5a42a-a049-4739-b0bd-a99902ebe7d5"
      }
    }
  }
}
